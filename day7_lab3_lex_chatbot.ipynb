    "## Part 5: Lambda Fulfillment Function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"üíª Lambda Fulfillment Function Example\\n\")\n",
    "print(\"=\"*80)\n",
    "\n",
    "print(\"\\n```python\")\n",
    "print(\"\"\"import json\n",
    "import boto3\n",
    "from decimal import Decimal\n",
    "\n",
    "dynamodb = boto3.resource('dynamodb')\n",
    "accounts_table = dynamodb.Table('customer-accounts')\n",
    "\n",
    "def lambda_handler(event, context):\n",
    "    \\\"\\\"\\\"Lex fulfillment function for SecureBankBot\\\"\\\"\\\"\n",
    "    \n",
    "    intent_name = event['sessionState']['intent']['name']\n",
    "    slots = event['sessionState']['intent']['slots']\n",
    "    \n",
    "    # Get customer ID from session attributes\n",
    "    customer_id = event['sessionAttributes'].get('customerId', 'DEMO-123')\n",
    "    \n",
    "    if intent_name == 'CheckBalance':\n",
    "        return handle_check_balance(customer_id, slots)\n",
    "    elif intent_name == 'TransferMoney':\n",
    "        return handle_transfer(customer_id, slots)\n",
    "    elif intent_name == 'LoanInquiry':\n",
    "        return handle_loan_inquiry(customer_id, slots)\n",
    "    else:\n",
    "        return close(event, 'Fulfilled', 'I\\'m not sure how to help with that.')\n",
    "\n",
    "def handle_check_balance(customer_id, slots):\n",
    "    \\\"\\\"\\\"Handle balance inquiry\\\"\\\"\\\"\n",
    "    account_type = slots['AccountType']['value']['interpretedValue']\n",
    "    \n",
    "    # Query DynamoDB for account balance\n",
    "    response = accounts_table.get_item(\n",
    "        Key={'customer_id': customer_id, 'account_type': account_type}\n",
    "    )\n",
    "    \n",
    "    if 'Item' in response:\n",
    "        balance = float(response['Item']['balance'])\n",
    "        message = f\"Your {account_type} account balance is ${balance:,.2f}.\"\n",
    "    else:\n",
    "        message = f\"I couldn't find a {account_type} account for you.\"\n",
    "    \n",
    "    return {\n",
    "        'sessionState': {\n",
    "            'dialogAction': {'type': 'Close'},\n",
    "            'intent': {\n",
    "                'name': 'CheckBalance',\n",
    "                'state': 'Fulfilled'\n",
    "            }\n",
    "        },\n",
    "        'messages': [{'contentType': 'PlainText', 'content': message}]\n",
    "    }\n",
    "\n",
    "def handle_transfer(customer_id, slots):\n",
    "    \\\"\\\"\\\"Handle money transfer\\\"\\\"\\\"\n",
    "    amount = float(slots['Amount']['value']['interpretedValue'])\n",
    "    from_account = slots['FromAccount']['value']['interpretedValue']\n",
    "    to_account = slots['ToAccount']['value']['interpretedValue']\n",
    "    \n",
    "    # Validate sufficient funds\n",
    "    from_response = accounts_table.get_item(\n",
    "        Key={'customer_id': customer_id, 'account_type': from_account}\n",
    "    )\n",
    "    \n",
    "    if 'Item' not in from_response:\n",
    "        return error_response('Source account not found')\n",
    "    \n",
    "    current_balance = float(from_response['Item']['balance'])\n",
    "    \n",
    "    if current_balance < amount:\n",
    "        return error_response(f'Insufficient funds. Available: ${current_balance:,.2f}')\n",
    "    \n",
    "    # Perform transfer (in transaction)\n",
    "    try:\n",
    "        # Debit from source\n",
    "        accounts_table.update_item(\n",
    "            Key={'customer_id': customer_id, 'account_type': from_account},\n",
    "            UpdateExpression='SET balance = balance - :amount',\n",
    "            ExpressionAttributeValues={':amount': Decimal(str(amount))}\n",
    "        )\n",
    "        \n",
    "        # Credit to destination\n",
    "        accounts_table.update_item(\n",
    "            Key={'customer_id': customer_id, 'account_type': to_account},\n",
    "            UpdateExpression='SET balance = balance + :amount',\n",
    "            ExpressionAttributeValues={':amount': Decimal(str(amount))}\n",
    "        )\n",
    "        \n",
    "        new_balance = current_balance - amount\n",
    "        message = (f\"Transfer completed! ${amount:,.2f} has been moved from \"\n",
    "                  f\"{from_account} to {to_account}. \"\n",
    "                  f\"Your new {from_account} balance is ${new_balance:,.2f}.\")\n",
    "        \n",
    "        return success_response(message)\n",
    "        \n",
    "    except Exception as e:\n",
    "        return error_response(f'Transfer failed: {str(e)}')\n",
    "\n",
    "def handle_loan_inquiry(customer_id, slots):\n",
    "    \\\"\\\"\\\"Handle loan inquiry\\\"\\\"\\\"\n",
    "    loan_type = slots['LoanType']['value']['interpretedValue']\n",
    "    amount = float(slots['Amount']['value']['interpretedValue'])\n",
    "    \n",
    "    # Calculate loan options\n",
    "    rates = {\n",
    "        'personal': 0.075,\n",
    "        'mortgage': 0.045,\n",
    "        'auto': 0.055,\n",
    "        'business': 0.065\n",
    "    }\n",
    "    \n",
    "    rate = rates.get(loan_type, 0.08)\n",
    "    \n",
    "    # Calculate monthly payments\n",
    "    term_3yr = calculate_payment(amount, rate, 36)\n",
    "    term_5yr = calculate_payment(amount, rate + 0.01, 60)\n",
    "    \n",
    "    message = (f\"For a ${amount:,.0f} {loan_type} loan, here are your options:\\n\\n\"\n",
    "              f\"‚Ä¢ 3-year term: ${term_3yr:,.0f}/month at {rate*100:.1f}% APR\\n\"\n",
    "              f\"‚Ä¢ 5-year term: ${term_5yr:,.0f}/month at {(rate+0.01)*100:.1f}% APR\\n\\n\"\n",
    "              f\"Would you like to start an application?\")\n",
    "    \n",
    "    return success_response(message)\n",
    "\n",
    "def calculate_payment(principal, rate, months):\n",
    "    \\\"\\\"\\\"Calculate monthly loan payment\\\"\\\"\\\"\n",
    "    monthly_rate = rate / 12\n",
    "    payment = principal * (monthly_rate * (1 + monthly_rate)**months) / \\\n",
    "              ((1 + monthly_rate)**months - 1)\n",
    "    return payment\n",
    "\n",
    "def success_response(message):\n",
    "    return {\n",
    "        'sessionState': {\n",
    "            'dialogAction': {'type': 'Close'},\n",
    "            'intent': {'state': 'Fulfilled'}\n",
    "        },\n",
    "        'messages': [{'contentType': 'PlainText', 'content': message}]\n",
    "    }\n",
    "\n",
    "def error_response(message):\n",
    "    return {\n",
    "        'sessionState': {\n",
    "            'dialogAction': {'type': 'Close'},\n",
    "            'intent': {'state': 'Failed'}\n",
    "        },\n",
    "        'messages': [{'contentType': 'PlainText', 'content': message}]\n",
    "    }\n",
    "\"\"\")\n",
    "print(\"```\\n\")\n",
    "print(\"=\"*80)"
   ]
