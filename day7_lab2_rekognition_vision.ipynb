    "print(\"üíª Production Integration Code Examples\\n\")\n",
    "print(\"=\"*80)\n",
    "\n",
    "print(\"\\n1Ô∏è‚É£ Complete KYC Verification Function:\\n\")\n",
    "print(\"\"\"```python\n",
    "import boto3\n",
    "from typing import Dict, Tuple\n",
    "\n",
    "def verify_kyc(id_image_s3: Dict, selfie_image_s3: Dict) -> Tuple[bool, Dict]:\n",
    "    \\\"\\\"\\\"Complete KYC verification using Rekognition\\\"\\\"\\\"\n",
    "    rekognition = boto3.client('rekognition')\n",
    "    \n",
    "    results = {\n",
    "        'id_text_extracted': False,\n",
    "        'id_face_detected': False,\n",
    "        'selfie_face_detected': False,\n",
    "        'faces_match': False,\n",
    "        'similarity_score': 0.0,\n",
    "        'approved': False\n",
    "    }\n",
    "    \n",
    "    try:\n",
    "        # Step 1: Extract text from ID\n",
    "        text_response = rekognition.detect_text(Image=id_image_s3)\n",
    "        results['id_text_extracted'] = len(text_response['TextDetections']) > 0\n",
    "        \n",
    "        # Step 2: Detect face in ID\n",
    "        id_face_response = rekognition.detect_faces(\n",
    "            Image=id_image_s3,\n",
    "            Attributes=['ALL']\n",
    "        )\n",
    "        results['id_face_detected'] = len(id_face_response['FaceDetails']) > 0\n",
    "        \n",
    "        # Step 3: Detect face in selfie\n",
    "        selfie_face_response = rekognition.detect_faces(\n",
    "            Image=selfie_image_s3,\n",
    "            Attributes=['ALL']\n",
    "        )\n",
    "        results['selfie_face_detected'] = len(selfie_face_response['FaceDetails']) > 0\n",
    "        \n",
    "        # Step 4: Compare faces\n",
    "        if results['id_face_detected'] and results['selfie_face_detected']:\n",
    "            compare_response = rekognition.compare_faces(\n",
    "                SourceImage=id_image_s3,\n",
    "                TargetImage=selfie_image_s3,\n",
    "                SimilarityThreshold=90\n",
    "            )\n",
    "            \n",
    "            if compare_response['FaceMatches']:\n",
    "                results['faces_match'] = True\n",
    "                results['similarity_score'] = compare_response['FaceMatches'][0]['Similarity']\n",
    "        \n",
    "        # Final decision\n",
    "        results['approved'] = all([\n",
    "            results['id_text_extracted'],\n",
    "            results['id_face_detected'],\n",
    "            results['selfie_face_detected'],\n",
    "            results['faces_match'],\n",
    "            results['similarity_score'] >= 90\n",
    "        ])\n",
    "        \n",
    "        return results['approved'], results\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"Error: {e}\")\n",
    "        return False, results\n",
    "\n",
    "# Usage\n",
    "id_image = {'S3Object': {'Bucket': 'kyc-docs', 'Name': 'id-123.jpg'}}\n",
    "selfie = {'S3Object': {'Bucket': 'kyc-docs', 'Name': 'selfie-123.jpg'}}\n",
    "\n",
    "approved, details = verify_kyc(id_image, selfie)\n",
    "print(f\"KYC Approved: {approved}\")\n",
    "print(f\"Similarity: {details['similarity_score']:.1f}%\")\n",
    "```\"\"\")\n",
    "\n",
    "print(\"\\n\" + \"=\"*80)\n",
    "print(\"\\n2Ô∏è‚É£ Lambda Function for Automated KYC:\\n\")\n",
    "print(\"\"\"```python\n",
    "import json\n",
    "import boto3\n",
    "\n",
    "rekognition = boto3.client('rekognition')\n",
    "dynamodb = boto3.resource('dynamodb')\n",
    "table = dynamodb.Table('kyc-verifications')\n",
    "\n",
    "def lambda_handler(event, context):\n",
    "    \\\"\\\"\\\"Process KYC verification triggered by S3 upload\\\"\\\"\\\"\n",
    "    \n",
    "    # Get S3 bucket and key from event\n",
    "    bucket = event['Records'][0]['s3']['bucket']['name']\n",
    "    key = event['Records'][0]['s3']['object']['key']\n",
    "    \n",
    "    # Extract application ID from key\n",
    "    app_id = key.split('/')[0]\n",
    "    \n",
    "    try:\n",
    "        # Perform verification\n",
    "        id_image = {'S3Object': {'Bucket': bucket, 'Name': f'{app_id}/id.jpg'}}\n",
    "        selfie = {'S3Object': {'Bucket': bucket, 'Name': f'{app_id}/selfie.jpg'}}\n",
    "        \n",
    "        approved, results = verify_kyc(id_image, selfie)\n",
    "        \n",
    "        # Store results in DynamoDB\n",
    "        table.put_item(\n",
    "            Item={\n",
    "                'application_id': app_id,\n",
    "                'timestamp': context.request_id,\n",
    "                'approved': approved,\n",
    "                'similarity_score': str(results['similarity_score']),\n",
    "                'details': json.dumps(results)\n",
    "            }\n",
    "        )\n",
    "        \n",
    "        return {\n",
    "            'statusCode': 200,\n",
    "            'body': json.dumps({\n",
    "                'application_id': app_id,\n",
    "                'approved': approved\n",
    "            })\n",
    "        }\n",
    "        \n",
    "    except Exception as e:\n",
    "        return {\n",
    "            'statusCode': 500,\n",
    "            'body': json.dumps({'error': str(e)})\n",
    "        }\n",
    "```\"\"\")\n",
    "\n",
    "print(\"\\n\" + \"=\"*80)"
